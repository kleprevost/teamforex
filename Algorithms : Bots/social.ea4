            <property name="string" type="string">block.name=Check Account Stoploss
block.id=CheckAccountStoploss
block.desc=Calculates maximum stoplosses of all open orders for the account. You can use it to check your maximum exposure for the account. Example: do not open new orders if your maximum exposure gets too high.
block.developer=Etasoft
block.version=1.0
block.genmode=0
block.builtin=1
block.type=account
block.1.body=double summ = 0;
block.2.body=for (int i=OrdersTotal()-1; i &gt;= 0; i--)
block.3.body=if (OrderSelect(i, SELECT_BY_POS, MODE_TRADES))
block.4.body={
block.5.body=    
block.6.body=    
block.7.body=    if (OrderType() == OP_BUY)
block.8.body=        summ += (Ask - OrderStopLoss()) / (PipValue*Point);
block.9.body=    if (OrderType() == OP_SELL)
block.10.body=        summ += (OrderStopLoss() - Bid) / (PipValue*Point);
